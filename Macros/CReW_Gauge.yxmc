              <Type>UpdateValueFormula</Type>
              <Description />
              <ToolId value="99" />
              <Expression>40*
(toNumber([Width])+toNumber([Height]))
/(2*7)</Expression>
              <Destination>17/Layers/Layer[@name='1']/LabelStyle/@Size</Destination>
              <Mode>Expression</Mode>
            </Action>
            <Action>
              <Type>UpdateValueFormula</Type>
              <Description />
              <ToolId value="100" />
              <Expression>iif(
round(
3*
(toNumber([Width])+toNumber([Height]))
/(2*7),1) == 0, 1,
round(
3*
(toNumber([Width])+toNumber([Height]))
/(2*7),1))</Expression>
              <Destination>17/Layers/Layer[@name='1']/PolygonStyle/@OutlineSize</Destination>
              <Mode>Expression</Mode>
            </Action>
